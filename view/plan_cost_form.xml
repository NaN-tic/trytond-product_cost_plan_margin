<data>
    <xpath expr="/form/field[@name='cost']" position="after">
        <label name="minimum"/>
        <field name="minimum"/>
        <label name="margin_percent"/>
        <group col="2" id="margin_percent">
            <field name="margin_percent" factor="100" xexpand="0"/>
            <label name="margin_percent" string="%" xalign="0.0" xexpand="1"/>
        </group>
        <label name="margin"/>
        <field name="margin"/>
    </xpath>
</data>
